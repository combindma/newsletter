{
    "name": "combindma/newsletter",
    "description": "Newsletter Subscription",
    "keywords": [
        "combind",
        "laravel",
        "newsletter"
    ],
    "homepage": "https://github.com/combindma/newsletter",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "hello@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "combindma/dashui": "dev-master",
        "combindma/flash": "^1.0",
        "elegantweb/sanitizer": "^1.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Newsletter\\": "src",
            "Combindma\\Newsletter\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Newsletter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-filter": "vendor/bin/phpunit --no-coverage --filter",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Newsletter\\NewsletterServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/combindma/dashui"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
